<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beachlist">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />


    <supports-screens></supports-screens>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeScreenAfterLogin"></activity>
        <activity android:name=".CreateAccount" />
        <activity android:name=".SelectedFriend"/>
        <activity android:name=".SelectedPendingFriend"/>
        <activity android:name=".ListingTitlePage"/>
        <activity android:name=".ListingDescriptionPage"/>
        <activity android:name=".ActiveListings"/>
        <activity android:name=".ListingReviewPage"/>
        <activity android:name=".ModifyListing"/>
        <activity android:name=".SelectedUser"/>
        <activity android:name=".SelectedOwnListing"/>
        <activity android:name=".SelectedItem"/>
        <activity android:name=".SelectedService"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FriendsListTab"></activity>
        <activity android:name=".PendingFriendsListTab"></activity>
    </application>

</manifest>